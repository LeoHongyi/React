"column":16},"source":"index.js","original":{"line":76,"column":16}},{"generated":{"line":94,"column":21},"source":"index.js","original":{"line":76,"column":15}},{"generated":{"line":94,"column":22},"source":"index.js","original":{"line":76,"column":2}},{"generated":{"line":94,"column":23},"source":"index.js","original":{"line":75,"column":19}},{"generated":{"line":94,"column":24},"source":"index.js","original":{"line":76,"column":23}},{"generated":{"line":95,"column":0},"source":"index.js","original":{"line":77,"column":1}}],"sources":{"index.js":"const Jreact = { //创建元素，组件\n  createElement\n}\n\nconst JreactDOM = { //用于去渲染，做一些其他的事情\n  render\n}\n\nfunction createElement(tag, attrs, ...children) { //es6语法中的数组，所有的子元素放数组里\n  return {\n    tag,\n    attrs,\n    children\n  }\n}\n\nfunction render(vnode, container) { //每次调用 render 时，先把之前的清空\n  container.innerHTML = ''\n  _render(vnode, container)\n}\n\nfunction _render(vnode, container) {\n  if (typeof vnode === 'string' || typeof vnode === 'number') { //如果是 string 或者 nubmer 都去创建文本节点\n    return container.appendChild(document.createTextNode(vnode))\n  }\n\n  if (typeof vnode === 'object') {\n    let dom = document.createElement(vnode.tag)\n    setAttribute(dom, vnode.attrs)\n    if (vnode.children && Array.isArray(vnode.children)) {\n      vnode.children.forEach(vnodeChild => {\n        _render(vnodeChild, dom) //记得这里是 _render , 这里的逻辑是不清空的\n      })\n    }\n\n    container.appendChild(dom)\n  }\n}\n\nfunction setAttribute(dom, attrs) {\n  for (let key in attrs) {\n    if (/^on/.test(key)) { //对事件绑定的处理，以 on 开头的，dom[onclick] = attrs[onClick]\n      dom[key.toLocaleLowerCase()] = attrs[key]\n    } else if (key === 'style') { //对 style 的处理\n      Object.assign(dom.style, attrs[key]) //新增的会赋值到 dom.style 上，同名的属性会覆盖\n    } else { //其他的直接作为 dom 的属性\n      dom[key] = attrs[key]\n    }\n  }\n}\n\nlet num = 0\nlet timer = null\nlet styleObj = {\n  color: 'red',\n  fontSize: '20px'\n}\n\nonStart() //一开始时执行\n\nfunction onStart() {\n  console.log('click me')\n  timer = setInterval(() => { //启动时，每秒钟计时一次，做一次渲染\n    num++\n    JreactDOM.render((\n      <div className=\"wrapper\">\n        <h1 style = { styleObj }>Number: { num }</h1>\n        <button onClick = { onStart }>start</button>\n        <button onClick = { onPause }>pause</button>\n      </div>\n    ), document.querySelector('#app'))\n  }, 1000)\n}\n\nfunction onPause() {\n  clearInterval(timer) //点击停止时，清除定时器\n}\n\n\n\n\n\n\n\n\n"},"lineCount":null}},"error":null,"hash":"374f50f8fa8de47b2d850003592dead7","cacheData":{"env":{}}}